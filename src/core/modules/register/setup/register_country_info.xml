<?xml version="1.0"?>
<SCHEMA CHARSET="utf8" ENGINE="InnoDB">
<TABLE NAME="register_country_info" LOCATION="local">
<STRUCTURE>CREATE TABLE `register_country_info` (
  `code` varchar(8) NOT NULL,
  `type` enum('danger','info','primary','success','warning') NOT NULL DEFAULT 'danger',
  `heading` varchar(100) NOT NULL,
  `short_description` varchar(255) NOT NULL,
  PRIMARY KEY (`code`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8</STRUCTURE>
<FIELD NAME="code">
<TYPE>varchar(8)</TYPE>
<COLLATION>utf8_general_ci</COLLATION>
<NULL>NO</NULL>
<KEY>PRI</KEY>
<PRIVALEGES>select,insert,update,references</PRIVALEGES>
</FIELD>
<FIELD NAME="type">
<TYPE>enum('danger','info','primary','success','warning')</TYPE>
<COLLATION>utf8_general_ci</COLLATION>
<NULL>NO</NULL>
<DEFAULT>danger</DEFAULT>
<PRIVALEGES>select,insert,update,references</PRIVALEGES>
</FIELD>
<FIELD NAME="heading">
<TYPE>varchar(100)</TYPE>
<COLLATION>utf8_general_ci</COLLATION>
<NULL>NO</NULL>
<PRIVALEGES>select,insert,update,references</PRIVALEGES>
</FIELD>
<FIELD NAME="short_description">
<TYPE>varchar(255)</TYPE>
<COLLATION>utf8_general_ci</COLLATION>
<NULL>NO</NULL>
<PRIVALEGES>select,insert,update,references</PRIVALEGES>
</FIELD>
</TABLE>
</SCHEMA>
